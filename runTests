for cmdFile in tests/*.cmd
do
    outFile=$(echo $cmdFile | sed 's/.cmd/.out/g')
    passed=true
    if [ "$(`cat $cmdFile`)" != "$(cat $outFile)" ] 
    then
        passed=false
        echo -e "\x1B[1;31mFAIL: $(cat $cmdFile)\x1B[0m"
    fi

done
if [ $passed == true ] 
then
    echo -e "\x1B[1;32mSUCCESS\x1B[0m"
fi